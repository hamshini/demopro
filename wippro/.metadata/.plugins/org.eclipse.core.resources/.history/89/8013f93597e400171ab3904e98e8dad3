
public class Videostore {
private String v[]=new String[10];
private int count=0;
int i;
Video o[]=new Video[10];
public void addVideo(String title)
{
	
	for(i=0;i<10;i++)
			{
				o[i]=new Video();
			}
	for(i=0;i<10;i++)
	{
		if(o[i].getTitle()==title)
		{
			System.out.println("The video is already exists");
			return;
		}
	}
	v[count]=title;
	o[count].setTitle(title);
	o[count].setFlag(true);
	count++;
}
public void checkout(String title)
{
	for(i=0;i<10;i++)
	{
		if(o[i].getTitle().equals(title))
			o[i].setFlag(false);
	}
}
public void retuened(String title)
{
	for(i=0;i<10;i++)
	{
		if(o[i].getTitle().equals(title))
			o[i].setFlag(true);
	}
}
public void receiveRating(String title,float rating)
{
	for(i=0;i<10;i++)
	{
		if(o[i].getTitle()==title)
			o[i].set
	}
}
}
