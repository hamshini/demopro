package com.number;

public class digitsum {
	int num=1234;
	
	void digitsum()
	{
		int sum=0;
		while(num>0)
		{
			int a=num%10;
			sum+=a;
			num=num/10;
		}
		System.out.println(sum);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		digitsum ob1=new digitsum();
		ob1.digitsum();
	}

}
