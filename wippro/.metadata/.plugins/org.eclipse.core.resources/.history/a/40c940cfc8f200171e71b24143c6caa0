import java.util.Scanner;
public class Number_1 {
	int split(int num)
	{
		while(num>0)
		{
			int num1=num%10;
			System.out.print(num1+" ");
			num=num/10;
			//return num1;
		}
		return num;
	}
	int sum(int num)
	{
		int sum=0;
		while(num>0)
		{
			int num1=num%10;
			sum=sum+num1;
			num=num/10;	
			//return num1;
		}
		System.out.println();
		System.out.println(sum);
		return sum;
	}
	int even_sum(int num)
	{
		int sum=0;
		while(num>0)
		{
			int num1=num%10;
			if(num1%2==0)
			{
				sum=sum+num1;
			}
			num=num/10;	
			//return num1;
		}
		//System.out.println();
		System.out.println(sum);
		return sum;
	}
	int odd_sum(int num)
	{
		int sum=0;
		while(num>0)
		{
			int num1=num%10;
			if(num1%2!=0)
			{
				sum=sum+num1;
			}
			num=num/10;	
			//return num1;
		}
		//System.out.println();
		System.out.println(sum);
		return sum;
	}
	int occurance(int num)
	{	
		int num1=0,count=1;
		int a[]=new int[4];
		int i=0;
		while(num>0)
		{
			num1=num%10;
			a[i]=num1;
			//System.out.println(a[i]);
			i++;
			num=num/10;
		}
		
			for(i=0;i<4;i++)
			{
				for(int j=i+1;j<4;j++)
				{
					if(a[i]==a[j])
					{
						count=count+1;
					}
				}
				System.out.println(a[i]+" "+count);
			}
		
		return count;
	}
	int cumulative_sum(int num)
	{
		int sum=0,sum1=0;
		while(num>0)
		{
			int num1=num%10;
			sum=sum+num1;
			num=num/10;	
			//return num1;
		}
		//System.out.println(sum);
		if(sum>9)
		{
			while(sum>0)
			{
				int re=sum%10;
				sum1=sum1+re;
				sum=sum/10;
			}
			System.out.println("the cumulative sum is:"+sum1);
		}
		else
		{
			System.out.println("the cumulative sum is:"+sum);
		}
		return sum;
	}
	int reverse(int num)
	{
		int output=0;
		while(num>0)
		{
			int num1=num%10;
			output=output*10+num1;
			num=num/10;	
			//return num1;
		}
		System.out.println(output);
		return output;
	}
	int cyclic_sum(int num)
	{
		int num1=0,sum=0,sum1=0;
		int a[]=new int[4];
		int i=0;
		while(num>0)
		{
			num1=num%10;
			a[i]=num1;
			//System.out.println(a[i]);
			i++;
			num=num/10;
		}
		for(i=0;i<a.length;i++)
		{
			for(int j=i+1;j<a.length;j++)
			{
				sum=sum+a[i];
				sum1=sum+a[j];
			}
		}
		System.out.println("the cyclic sum is"+sum);
		return sum;
	}
	int duplicate(int num)
	{
		int num1=0,count=1;
		int a[]=new int[4];
		int i=0;
		while(num>0)
		{
			num1=num%10;
			a[i]=num1;
			//System.out.println(a[i]);
			i++;
			num=num/10;
		}
		
			for(i=0;i<4;i++)
			{
				for(int j=i+1;j<4;j++)
				{
					if(a[i]==a[j])
					{
						count=count+1;
					}
				}
				System.out.println("the non-duplicate elements are:");
				if(count==1)
				{
					System.out.println(a[i]);
				}
			}
		
		return count;
	}
	public static void main(String args[])
	{
		Scanner obj=new Scanner(System.in);
		int num=obj.nextInt();
		Number_1 obj1=new Number_1();
		obj1.split(num);
		obj1.sum(num);
		obj1.even_sum(num);
		obj1.odd_sum(num);
		obj1.occurance(num);
		obj1.cumulative_sum(num);
		obj1.reverse(num);
		obj1.cyclic_sum(num);
	}
}
