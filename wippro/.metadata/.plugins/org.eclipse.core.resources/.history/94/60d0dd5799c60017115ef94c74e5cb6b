import java.util.Scanner;

public class MedianFind {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		int m = in.nextInt();
		int median,j=n;
		int a[] = new int[n];
		int b[] = new int[m];
		int temp[] = new int[n + m];
		for (int i = 0; i < n; i++) {
			a[i] = in.nextInt();
			temp[i]=a[i];
		}
		for (int i = 0; i < m; i++) {
			b[i] = in.nextInt();
			temp[j]=b[i];
			j++;
		}
		if (n == m) {
			median = ((a[n] + b[0]) / 2);
		} else {
			for(int i=0;i<(n+m);i++){
			System.out.println(temp[i]);
			}

		}
	}

}
